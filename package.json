{
  "name": "nodejs-epam-hw-tasks",
  "version": "2.0.0",
  "description": "NodeJS Global Mentoring Program 2020Q3",
  "main": "src/hw02/index.ts",
  "scripts": {
    "hw01:task1": "nodemon src/hw01/task1.js",
    "hw01:task1:babel": "nodemon --exec babel-node --ignore 'csv/*.*' src/hw01/task1.babel.js",
    "hw01:task2": "nodemon --ignore 'csv/*.*' src/hw01/task2.js",
    "hw01:task2:babel": "nodemon --exec babel-node --ignore 'csv/*.*' src/hw01/task2.babel.js",
    "hw02:start": "nodemon --watch 'src/hw02' --ext 'ts,json' --exec 'ts-node -r tsconfig-paths/register --project tsconfig.hw02.json' src/hw02/index.ts",
    "lint": "eslint . --ext '.js,.ts'",
    "start": "nodemon --watch 'src/hw03' --ext 'ts,json' --exec 'ts-node -r tsconfig-paths/register --project tsconfig.json' src/hw03/index.ts",
    "typeorm-cli": "ts-node --project tsconfig.json -r tsconfig-paths/register node_modules/.bin/typeorm --config src/hw03/ormconfig.env",
    "db:create:schema": "cross-env TYPEORM_DATABASE=postgres yarn run typeorm-cli query 'CREATE DATABASE ngmp20_h3;'",
    "db:drop:schema": "cross-env TYPEORM_DATABASE=postgres yarn run typeorm-cli query 'DROP DATABASE ngmp20_h3;'",
    "db:migration:create": "yarn run typeorm-cli migration:create -n",
    "db:migration:generate": "yarn run typeorm-cli migration:generate -n",
    "db:migration:revert": "yarn run typeorm-cli migration:revert",
    "db:migration:run": "yarn run typeorm-cli migration:run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elbertcastaneda/nodejs-epam-hw-tasks.git"
  },
  "keywords": [],
  "author": "Elbert Casta√±eda<elbert.castaneda@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/elbertcastaneda/nodejs-epam-hw-tasks/issues"
  },
  "homepage": "https://github.com/elbertcastaneda/nodejs-epam-hw-tasks#readme",
  "engines": {
    "node": ">=10",
    "yarn": ">=1.20"
  },
  "dependencies": {
    "class-validator": "^0.12.2",
    "cors": "^2.8.5",
    "csvtojson": "^2.0.10",
    "express": "^4.17.1",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.28",
    "uuid": "^8.3.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.5",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.8",
    "@types/node": "^14.11.5",
    "@types/uuid": "^8.3.0",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.10.0",
    "eslint-config-airbnb-typescript": "^11.0.0",
    "eslint-plugin-import": "^2.22.1",
    "nodemon": "^2.0.4",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.3"
  }
}
