{
  "name": "nodejs-epam-hw-tasks",
  "version": "2.0.0",
  "description": "NodeJS Global Mentoring Program 2020Q3",
  "main": "src/hw02/index.ts",
  "scripts": {
    "hw01:task1": "nodemon src/hw01/task1.js",
    "hw01:task1:babel": "nodemon --exec babel-node --ignore 'csv/*.*' src/hw01/task1.babel.js",
    "hw01:task2": "nodemon --ignore 'csv/*.*' src/hw01/task2.js",
    "hw01:task2:babel": "nodemon --exec babel-node --ignore 'csv/*.*' src/hw01/task2.babel.js",
    "hw02:start": "nodemon --watch 'src/hw02' --ext 'ts,json' --exec 'ts-node -r tsconfig-paths/register --project tsconfig.hw02.json' src/hw02/index.ts",
    "lint": "eslint . --ext '.js,.ts'",
    "start": "nodemon --watch 'src/hw03' --ext 'ts,json' --exec 'ts-node -r tsconfig-paths/register --project tsconfig.json' src/hw03/index.ts",
    "start:debug": "nodemon --watch --inspect=5858 'src/hw03' --ext 'ts,json' --exec 'ts-node -r tsconfig-paths/register --project tsconfig.json' src/hw03/index.ts",
    "typeorm-cli": "ts-node --project tsconfig.json -r tsconfig-paths/register node_modules/.bin/typeorm",
    "db:create:schema": "cross-env TYPEORM_DATABASE=postgres yarn run typeorm-cli -d src/hw03/config/typeorm.config.ts query 'CREATE DATABASE ngmp20;'",
    "db:drop:schema": "cross-env TYPEORM_DATABASE=postgres yarn run typeorm-cli -d src/hw03/config/typeorm.config.ts query 'DROP DATABASE ngmp20;'",
    "db:migration:create": "yarn run typeorm-cli migration:create",
    "db:migration:generate": "yarn run typeorm-cli migration:generate -d src/hw03/config/typeorm.config.ts",
    "db:migration:revert": "yarn run typeorm-cli migration:revert -d src/hw03/config/typeorm.config.ts",
    "db:migration:run": "yarn run typeorm-cli migration:run -d src/hw03/config/typeorm.config.ts",
    "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "start:build": "NODE_PATH=build/src/hw03/ node ./build/src/hw03/index.js "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elbertcastaneda/nodejs-epam-hw-tasks.git"
  },
  "keywords": [],
  "author": "Elbert Casta√±eda<elbert.castaneda@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/elbertcastaneda/nodejs-epam-hw-tasks/issues"
  },
  "homepage": "https://github.com/elbertcastaneda/nodejs-epam-hw-tasks#readme",
  "engines": {
    "node": ">=10",
    "yarn": ">=1.20"
  },
  "dependencies": {
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csvtojson": "^2.0.10",
    "express": "^4.18.1",
    "helmet": "^6.0.0",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.4",
    "node-rsa": "^1.1.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.9",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.19.1",
    "@babel/node": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@rollup/plugin-typescript": "^8.5.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.7.18",
    "@types/node-rsa": "^1.1.1",
    "@types/passport": "^1.0.11",
    "@types/passport-jwt": "^3.0.6",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.23.1",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.7.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.3"
  }
}
