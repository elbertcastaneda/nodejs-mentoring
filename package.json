{
  "name": "nodejs-mentoring",
  "version": "2.0.0",
  "description": "NodeJS Mentoring Program",
  "main": "src/module02/index.ts",
  "scripts": {
    "mdl01:task1": "nodemon src/module01/task1.js",
    "mdl01:task1:babel": "nodemon --exec babel-node --ignore 'csv/*.*' src/module01/task1.babel.js",
    "mdl01:task2": "nodemon --ignore 'csv/*.*' src/module01/task2.js",
    "mdl01:task2:babel": "nodemon --exec babel-node --ignore 'csv/*.*' src/module01/task2.babel.js",
    "mdl02:start": "nodemon --watch 'src/module02' --ext 'ts,json' --exec 'ts-node -r tsconfig-paths/register --project tsconfig.mdl02.json' src/module02/index.ts",
    "lint": "eslint . --ext '.js,.ts'",
    "start": "nodemon --watch 'src/module03' --ext 'ts,json' --exec 'ts-node -r tsconfig-paths/register --project tsconfig.json' src/module03/index.ts",
    "start:debug": "nodemon --watch --inspect=5858 'src/module03' --ext 'ts,json' --exec 'ts-node -r tsconfig-paths/register --project tsconfig.json' src/module03/index.ts",
    "typeorm-cli": "ts-node --project tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "db:schema:create": "cross-env TYPEORM_DATABASE=postgres npm run typeorm-cli -d src/module03/config/typeorm.config.ts query \"CREATE DATABASE nmp;\"",
    "db:schema:drop": "cross-env TYPEORM_DATABASE=postgres npm run typeorm-cli -d src/module03/config/typeorm.config.ts query \"DROP DATABASE nmp;\"",
    "db:migration:create": "npm run typeorm-cli migration:create",
    "db:migration:generate": "npm run typeorm-cli migration:generate -d src/module03/config/typeorm.config.ts",
    "db:migration:revert": "npm run typeorm-cli migration:revert -d src/module03/config/typeorm.config.ts",
    "db:migration:run": "npm run typeorm-cli migration:run -d src/module03/config/typeorm.config.ts",
    "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "build:webpack": "rimraf ./dist && npm run build && webpack && rimraf ./dist/module03 && cd ./dist && npm install",
    "pack": "npm run build:webpack && cd ./dist/ && pkg . && rimraf node_modules index.js package.json package-lock.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elbertcastaneda/nodejs-mentoring.git"
  },
  "keywords": [],
  "author": "Elbert Casta√±eda<elbert.castaneda@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/elbertcastaneda/nodejs-mentoring/issues"
  },
  "homepage": "https://github.com/elbertcastaneda/nodejs-mentoring#readme",
  "engines": {
    "node": ">=10",
    "npm": ">=1.20"
  },
  "dependencies": {
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csvtojson": "^2.0.10",
    "express": "^4.18.2",
    "helmet": "^6.0.0",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.4",
    "node-rsa": "^1.1.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.10",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.3",
    "@babel/node": "^7.19.1",
    "@babel/preset-env": "^7.19.4",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.8.4",
    "@types/node-rsa": "^1.1.1",
    "@types/passport": "^1.0.11",
    "@types/passport-jwt": "^3.0.7",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.25.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "generate-package-json-webpack-plugin": "^2.6.0",
    "nodemon": "^2.0.20",
    "pkg": "^5.8.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.7.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-node-externals": "^3.0.0"
  }
}
